{
    "name": "simon-schubert/terry-api",
    "type": "symfony-bundle",
    "description": "TerryApiBundle is a Symfony Bundle to create REST APIs. While you can focus on your data model, business logic and persistance layer implementations, TerryApiBundle handles serialization, validation and HTTP things like headers or status codes.",
    "license": "MIT",
    "require": {
        "php": "7.4.*",
        "doctrine/annotations": "^1.8",
        "symfony/config": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/http-foundation": "^5.0",
        "symfony/serializer-pack": "^1.0",
        "symfony/validator": "^5.0",
        "symfony/expression-language": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.0",
        "squizlabs/php_codesniffer": "^3.5.3",
        "infection/infection": "^0.16.0",
        "vimeo/psalm": "3.7.0",
        "phake/phake": "^3.1",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "TerryApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TerryApiBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpcs",
            "phpunit",
            "psalm",
            "infection"
        ]
    }
}
